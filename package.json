{
  "name": "angular2-sandbox",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/devcube/angular2-sandbox",
  "scripts": {
    "clean:wwwroot": "rimraf server/wwwroot",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:wwwroot",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "stats:prod": "webpack --config webpack.prod.config.js --profile --json > webpack.json",
    "server:dev": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached  --content-base src/",
    "start": "npm run server:dev"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "es7-reflect-metadata": "^1.5.5",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.14",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.0"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^0.3.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "es6-promise-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "replace": "^0.3.0",
    "rimraf": "^2.5.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "sweetalert": "^1.1.3",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.5.5",
    "tsconfig-lint": "^0.5.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typescript": "1.7.5",
    "typings": "^0.6.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-load-plugins": "^0.1.2",
    "webpack-md5-hash": "^0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devcube/angular2-sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/devcube/angular2-sandbox/issues"
  }
}
